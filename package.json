		"prettier-plugin-svelte": "^3.3.3",
		"prisma": "^6.2.1",
		"svelte": "^5.19.3",
		"svelte-check": "^4.1.7",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
